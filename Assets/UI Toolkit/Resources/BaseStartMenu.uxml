        <ui:VisualElement name="exit" class="not_selected" style="top: 50%; width: 40%; height: 10%; align-items: center; justify-content: center; font-size: 20px; -unity-font: resource(&apos;Fonts &amp; Materials/press-start-2p-font/PressStart2P-vaV7&apos;);">
