            <ui:Label text="The Dream Smp Fan JRPG" display-tooltip-when-elided="true" style="align-items: center; justify-content: center; font-size: 30%;" />
        </ui:VisualElement>
        <ui:VisualElement name="Start" class="selected" style="top: 20%; width: 40%; height: 10%; align-items: center; justify-content: center; -unity-font: resource(&apos;Fonts &amp; Materials/press-start-2p-font/PressStart2P-vaV7&apos;); font-size: 20px;">
