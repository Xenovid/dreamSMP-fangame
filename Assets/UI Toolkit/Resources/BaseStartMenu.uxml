        <ui:VisualElement name="Options" class="not_selected" style="top: 30%; width: 40%; height: 10%; justify-content: center; align-items: center; font-size: 20px; -unity-font: resource(&apos;Fonts &amp; Materials/press-start-2p-font/PressStart2P-vaV7&apos;);">
